<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Plugins">
        <!-- Register XML module catalog plugin -->
        <folder name="XML">
            <folder name="UserCatalogs">
                <file name="de-fub-mapsforge-xmlcatalogmodule-MapsForgeCatalogProvider.instance">
                    <attr name="instanceCreate" newvalue="fu.berlin.mapsforge.xmlcatalogmodule.MapsForgeCatalogProvider"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.xml.catalog.spi.CatalogReader"/>
                </file>
            </folder>
        </folder>
    </folder>
    <folder name="MapsForge">
        <folder name="Catalog"/>
    </folder>
</filesystem>
